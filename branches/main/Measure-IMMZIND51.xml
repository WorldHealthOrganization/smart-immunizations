        <extension url="statement">
          <valueString value="/**&#xA; * @dataElement All Doses Administered to Patient to patient ordered newest to oldest&#xA; */&#xA;define &quot;Doses Administered to Patient&quot;:&#xA;  [Immunization] I&#xA;    where I.status = 'completed'&#xA;    sort by date from (occurrence as FHIR.dateTime) desc"/>
        </extension>
        <extension url="displaySequence">
          <valueInteger value="4"/>
        </extension>
      </extension>
      <extension url="http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition">
        <extension url="libraryName">
          <valueString value="IMMZCommon"/>
        </extension>
        <extension url="name">
          <valueString value="BCG Doses Administered to Patient"/>
        </extension>
        <extension url="statement">
          <valueString value="/**&#xA; * @dataElement BCG Doses Administered to Patient&#xA; */&#xA;define &quot;BCG Doses Administered to Patient&quot;:&#xA;  &quot;Doses Administered to Patient&quot; I &#xA;  where &#xA;    I.vaccineCode in IMMZc.&quot;BCG Vaccine&quot;"/>
        </extension>
        <extension url="displaySequence">
          <valueInteger value="5"/>
        </extension>
      </extension>
      <extension url="http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition">
        <extension url="libraryName">
          <valueString value="IMMZIND51"/>
        </extension>
        <extension url="name">
          <valueString value="Patient Should Complete BCG to MCV During Measurement Period"/>
        </extension>
        <extension url="statement">
          <valueString value="/**&#xA; * Patient should be completing DTP during measurement period&#xA; */&#xA;define &quot;Patient Should Complete BCG to MCV During Measurement Period&quot;:&#xA;&#x9;exists(&#xA;&#x9;&#x9;IMMZCom.&quot;BCG Doses Administered to Patient&quot; I &#xA;&#x9;&#x9;where &#xA;&#x9;&#x9;&#x9;I.occurrence before start of &quot;Measurement Period&quot;&#xA;&#x9;)&#xA;&#x9;and &#xA;&#x9;exists(&#xA;&#x9;&#x9;[ImmunizationRecommendation] IR &#xA;&#x9;&#x9;where &#xA;&#x9;&#x9;&#x9;exists(&#xA;&#x9;&#x9;&#x9;&#x9;IR.recommendation RC where&#xA;&#x9;&#x9;&#x9;&#x9;exists(RC.dateCriterion DC where date from DC.value during &quot;Measurement Period&quot;)&#xA;&#x9;&#x9;&#x9;&#x9;and RC.doseNumber = 1&#xA;&#x9;&#x9;&#x9;&#x9;and RC.vaccineCode in IMMZc.&quot;MCV Vaccine&quot;&#xA;&#x9;&#x9;)&#xA;&#x9;)"/>
        </extension>
        <extension url="displaySequence">
          <valueInteger value="6"/>
        </extension>
      </extension>
