 */
define &quot;Should vaccinate patient for Japanese Encephalitis because 1 dose in a 2 dose scheme&quot;:
	</a:s>
               <a:s r="40">
                  <a:s r="37">
                     <a:s r="31">
                        <a:s r="25">
                           <a:s r="23">
                              <a:s r="22">
