                           <a:s r="24"> = 1</a:s>
                        </a:s>
                        <a:s>
	and </a:s>
                        <a:s r="30">
                           <a:s r="27">
                              <a:s r="26">
                                 <a:s>IMMZCom</a:s>
                              </a:s>
                              <a:s>.</a:s>
                              <a:s r="27">
                                 <a:s>&quot;Date Last JE Dose Administered to Patient&quot;</a:s>
                              </a:s>
                           </a:s>
                           <a:s> </a:s>
                           <a:s r="30">
                              <a:s>more than </a:s>
                              <a:s r="29">
                                 <a:s>4 week</a:s>
                              </a:s>
                              <a:s> before</a:s>
                           </a:s>
                           <a:s> </a:s>
                           <a:s r="28">
                              <a:s>Today()</a:s>
                           </a:s>
                        </a:s>
                     </a:s>
                     <a:s>
	and </a:s>
                     <a:s r="36">
                        <a:s r="33">
                           <a:s r="32">
                              <a:s>IMMZCom</a:s>
                           </a:s>
                           <a:s>.</a:s>
                           <a:s r="33">
                              <a:s>&quot;Type of Last JE Dose Administered to Patient&quot;</a:s>
                           </a:s>
                        </a:s>
                        <a:s> in </a:s>
                        <a:s r="35">
                           <a:s r="34">
                              <a:s>IMMZc</a:s>
                           </a:s>
                           <a:s>.</a:s>
                           <a:s r="35">
                              <a:s>&quot;Inactivated Vero cell-derived Japanese Encephalitis vaccines&quot;</a:s>
                           </a:s>
                        </a:s>
                     </a:s>
                  </a:s>
                  <a:s>
	and </a:s>
                  <a:s r="39">
                     <a:s r="38">
                        <a:s>IMMZCon</a:s>
                     </a:s>
                     <a:s>.</a:s>
                     <a:s r="39">
                        <a:s>&quot;Country recognizes JE as a health priority&quot;</a:s>
                     </a:s>
                  </a:s>
               </a:s>
            </a:s>
         </annotation>
         <expression localId="40" locator="47:2-50:57" xsi:type="And">
            <operand localId="37" locator="47:2-49:131" xsi:type="And">
               <operand localId="31" locator="47:2-48:88" xsi:type="And">
                  <operand localId="25" locator="47:2-47:57" xsi:type="Equal">
                     <operand localId="23" locator="47:2-47:53" name="Number of JE Doses Administered to Patient" libraryName="IMMZCom" xsi:type="ExpressionRef"/>
                     <operand localId="24" locator="47:57" valueType="t:Integer" value="1" xsi:type="Literal"/>
                  </operand>
                  <operand localId="30" locator="48:6-48:88" xsi:type="Before">
                     <operand localId="27" locator="48:6-48:56" name="Date Last JE Dose Administered to Patient" libraryName="IMMZCom" xsi:type="ExpressionRef"/>
                     <operand locator="48:82-48:88" xsi:type="Subtract">
                        <operand localId="28" locator="48:82-48:88" xsi:type="Today"/>
                        <operand localId="29" locator="48:68-48:73" value="4" unit="week" xsi:type="Quantity"/>
                     </operand>
                  </operand>
               </operand>
               <operand localId="36" locator="49:6-49:131" xsi:type="InValueSet">
                  <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand localId="33" locator="49:6-49:59" name="Type of Last JE Dose Administered to Patient" libraryName="IMMZCom" xsi:type="ExpressionRef"/>
                  </code>
                  <valueset localId="35" locator="49:64-49:131" name="Inactivated Vero cell-derived Japanese Encephalitis vaccines" libraryName="IMMZc"/>
               </operand>
            </operand>
            <operand localId="39" locator="50:6-50:57" name="Country recognizes JE as a health priority" libraryName="IMMZCon" xsi:type="ExpressionRef"/>
         </expression>
      </def>
