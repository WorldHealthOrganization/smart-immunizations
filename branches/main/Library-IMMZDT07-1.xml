                                 <a:s r="48"> = 1</a:s>
                              </a:s>
                              <a:s>)</a:s>
                           </a:s>
                           <a:s> and 
		</a:s>
                           <a:s r="53">
                              <a:s>(</a:s>
                              <a:s r="53">
                                 <a:s r="50">
                                    <a:s>&quot;Date last Rotavirus dose given&quot;</a:s>
                                 </a:s>
                                 <a:s> </a:s>
                                 <a:s r="53">
                                    <a:s>more than </a:s>
                                    <a:s r="52">
                                       <a:s>4 'weeks'</a:s>
                                    </a:s>
                                    <a:s> before</a:s>
                                 </a:s>
                                 <a:s> </a:s>
                                 <a:s r="51">
