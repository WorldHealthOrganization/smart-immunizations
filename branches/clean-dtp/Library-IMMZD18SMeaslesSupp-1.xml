                              </a:s>
                              <a:s>.</a:s>
                              <a:s r="19">
                                 <a:s>&quot;MCV Doses Administered to Patient&quot;</a:s>
                              </a:s>
                           </a:s>
                           <a:s>)</a:s>
                        </a:s>
                        <a:s>.</a:s>
                        <a:s r="21">
                           <a:s>occurrence</a:s>
                        </a:s>
                     </a:s>
                     <a:s> + </a:s>
                     <a:s r="22">
                        <a:s>4 week</a:s>
                     </a:s>
                  </a:s>
                  <a:s r="24">
	else null</a:s>
               </a:s>
            </a:s>
