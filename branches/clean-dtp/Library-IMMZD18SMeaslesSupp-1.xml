@dynamicValue: Measles supplementary dose Due Date
@pseudocode: &quot;Date when the primary vaccination series completed&quot; + 4 'week'
*/
define &quot;Measles supplementary dose Due Date&quot;:
	</a:s>
               <a:s r="25">
                  <a:s>if </a:s>
                  <a:s r="17">
                     <a:s>&quot;Measles supplementary dose&quot;</a:s>
                  </a:s>
