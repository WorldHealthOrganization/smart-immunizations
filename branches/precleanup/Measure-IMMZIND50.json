      {
        "url" : "statement",
        "valueString" : "/*\n * Disaggregator: Geographic Region\n */\ndefine \"Geographic Region Stratifier\":\n\tIMMZIndCom.\"By Geographic Region Stratifier\""
      },
      {
        "url" : "displaySequence",
