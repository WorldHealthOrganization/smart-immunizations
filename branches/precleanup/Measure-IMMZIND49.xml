        </extension>
        <extension url="statement">
          <valueString value="/**&#xA; * By Administrative Gender of Patient Stratifier&#xA; */&#xA;define &quot;By Geographic Region Stratifier&quot;:&#xA;    First(Patient.address A where A.use in { 'home' }).state"/>
