      {
        "url" : "statement",
        "valueString" : "// todo: fill in logic\n\n/*\n * Disaggregator: Administrative Gender (or Biological Sex)\n */\ndefine \"Gender Stratifier\":\n\tIMMZIndCom.\"By Administrative Gender Stratifier\""
      },
