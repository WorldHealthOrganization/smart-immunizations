      },
      {
        "url" : "statement",
        "valueString" : "// todo: fill in logic\n\n/*\n * Disaggregator: Geographic Region\n */\ndefine \"Geographic Region Stratifier\":\n\tIMMZIndCom.\"By Geographic Region Stratifier\""
      },
      {
        "url" : "displaySequence",
