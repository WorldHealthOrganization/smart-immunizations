        "valueString" : "Doses Administered to Patient"
      },
      {
        "url" : "statement",
        "valueString" : "/**\n * @dataElement All Doses Administered to Patient to patient ordered newest to oldest\n */\ndefine \"Doses Administered to Patient\":\n  [Immunization] I\n    where I.status = 'completed'\n    sort by date from (occurrence as FHIR.dateTime) desc"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 4
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
        "url" : "libraryName",
        "valueString" : "IMMZCommon"
      },
      {
        "url" : "name",
        "valueString" : "HepB Doses Administered to Patient"
      },
      {
        "url" : "statement",
        "valueString" : "/**\n * @dataElement HepB containing Doses Administered to Patient\n */\ndefine \"HepB Doses Administered to Patient\":\n  \"Doses Administered to Patient\" I\n  where\n    I.vaccineCode in IMMZc.\"HepB Vaccine\""
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 5
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
        "url" : "libraryName",
        "valueString" : "IMMZIndicatorCommon"
      },
      {
        "url" : "name",
        "valueString" : "HepB Birth Doses Administered to Patient During Measurement Period"
      },
      {
        "url" : "statement",
        "valueString" : "/*\n * HepB Doses administered to the patient during the measurement period\n */\ndefine \"HepB Birth Doses Administered to Patient During Measurement Period\":\n    IMMZCom.\"HepB Doses Administered to Patient\" I\n\t\twhere IMMZCom.ToDate(I.occurrence) during \"Measurement Period\"\n        and (singleton from I.protocolApplied).doseNumber = 0"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 6
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
        "url" : "libraryName",
        "valueString" : "IMMZIND53"
      },
      {
        "url" : "name",
        "valueString" : "numerator"
      },
      {
        "url" : "statement",
        "valueString" : "/*\n * Numerator: Number of administrations of vaccines containing a Hepatitis B component at birth (dose sequence 0)\n * Numerator Computation: COUNT immunization events WHERE administered product is a HepB (IMMZ.A1.DE6) during reporting period and dose sequence = 0\n */\ndefine \"numerator\":\n\texists(IMMZIndCom.\"HepB Birth Doses Administered to Patient During Measurement Period\")"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 7
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
        "url" : "libraryName",
        "valueString" : "IMMZIndicatorCommon"
      },
      {
        "url" : "name",
        "valueString" : "Patient Born During Measurement Period"
      },
      {
        "url" : "statement",
        "valueString" : "/** \n * Patient was born during measurement period\n */\ndefine \"Patient Born During Measurement Period\":\n    Patient.birthDate during \"Measurement Period\""
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 8
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
        "url" : "libraryName",
        "valueString" : "IMMZIndicatorCommon"
      },
      {
        "url" : "name",
        "valueString" : "Patient Deceased During Measurement Period"
      },
      {
        "url" : "statement",
        "valueString" : "/** \n * @dataElement Patient is deceased\n */\ndefine \"Patient Deceased During Measurement Period\":\n  case \n    when Patient.deceased is FHIR.boolean then Patient.deceased as FHIR.boolean\n    when Patient.deceased is FHIR.dateTime then Patient.deceased as FHIR.dateTime before end of \"Measurement Period\"\n    else false\n  end"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 9
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
        "url" : "libraryName",
        "valueString" : "IMMZIND53"
      },
      {
        "url" : "name",
        "valueString" : "denominator"
      },
      {
        "url" : "statement",
        "valueString" : "/*\n * Denominator: Number of registered, surviving infants whose ages is less than 12 months during reporting period.\n * Denominator Computation: COUNT Patient WHERE age at reporting period < 12 months AND deceasedDate is not populated.\n */\ndefine \"denominator\":\n\tIMMZIndCom.\"Patient Born During Measurement Period\"\n\tand not (IMMZIndCom.\"Patient Deceased During Measurement Period\")"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 10
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
