      {
        "url" : "name",
        "valueString" : "Infant By Age Stratifier"
      },
      {
        "url" : "statement",
        "valueString" : "/** \n * Infant disaggregations by age\n */\ndefine \"Infant By Age Stratifier\":\n    case \n        when AgeInYearsAt(start of \"Measurement Period\") < 1 then \"P0Y--P1Y\"\n        when AgeInYearsAt(start of \"Measurement Period\") >= 1 then \"P1Y--P9999Y\"\n        else null\n    end"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 0
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
