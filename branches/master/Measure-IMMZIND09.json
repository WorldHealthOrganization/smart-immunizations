      {
        "url" : "name",
        "valueString" : "Adolescent By Age Stratifier"
      },
      {
        "url" : "statement",
        "valueString" : "/** \n * Toddler By Age Stratifiers\n */\ndefine \"Adolescent By Age Stratifier\":\n    case \n        when AgeInYearsAt(start of \"Measurement Period\") < 9 then \"P0Y--P9Y\"\n        when AgeInYearsAt(start of \"Measurement Period\") <= 14 then \"P9Y--P14Y\"\n        when AgeInYearsAt(start of \"Measurement Period\") > 14 then \"P15Y--P9999Y\"\n        else null\n    end"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 7
