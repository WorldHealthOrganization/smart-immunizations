      },
      {
        "url" : "name",
        "valueString" : "HPV Doses Administered to Patient"
      },
      {
        "url" : "statement",
        "valueString" : "/**\n * @dataElement HPV containing Doses Administered to Patient\n */\ndefine \"HPV Doses Administered to Patient\":\n  \"Doses Administered to Patient\" I\n  where\n    I.vaccineCode in IMMZc.\"HPV Vaccine\""
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 3
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
        "url" : "libraryName",
        "valueString" : "IMMZIndicatorCommon"
      },
      {
        "url" : "name",
        "valueString" : "HPV Doses Administered to Patient During Measurement Period"
      },
      {
        "url" : "statement",
        "valueString" : "/**\n * HPV Doses Administered to Patient\n */\ndefine \"HPV Doses Administered to Patient During Measurement Period\":\n    IMMZCom.\"HPV Doses Administered to Patient\" I \n    where IMMZCom.ToDate(I.occurrence) during \"Measurement Period\""
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 4
      }],
      "url" : "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"
    },
    {
      "extension" : [{
