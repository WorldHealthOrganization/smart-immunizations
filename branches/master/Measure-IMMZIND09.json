      },
      {
        "url" : "name",
        "valueString" : "Dose Sequence Stratifier"
      },
      {
        "url" : "statement",
        "valueString" : "// TODO: Write logic here \n\n/*\n * Disaggregator: Dose Sequence (1, 2, 3)\n */\ndefine \"Dose Sequence Stratifier\":\n\tIMMZIndCom.\"HPV Doses Administered to Patient During Measurement Period\" I\n\treturn (singleton from I.protocolApplied).doseNumber"
      },
      {
        "url" : "displaySequence",
        "valueInteger" : 6
