            <a:s r="23">
               <a:s>/*
@output: Measles MCV0 dose
@trigger: Child's birth
*/
define &quot;Measles MCV0 dose&quot;:
	</a:s>
               <a:s r="22">
                  <a:s r="13">
                     <a:s r="11">
                        <a:s>Count(</a:s>
                        <a:s r="10">
                           <a:s r="9">
                              <a:s>input</a:s>
