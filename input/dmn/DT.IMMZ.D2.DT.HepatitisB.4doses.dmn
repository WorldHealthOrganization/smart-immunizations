          <dmn:text>No hepatitis B primary series doses were administered</dmn:text>
        </dmn:inputEntry>
        <dmn:inputEntry>
          <dmn:description>Today's date − "Date of birth" &lt; Member States defined lower limit</dmn:description>
          <dmn:text>Clients age is less than {Member States defined lower limit}</dmn:text>
