  <dmn:decision id="DT-IMMZ.D2.DT.Measles.Supplementarydose" name="DT-IMMZ.D2.DT.Measles.Supplementarydose">
    <dmn:question>Determine if the client is due for a measles vaccination according to the national immunization schedule</dmn:question>
    <dmn:usingTask href="http://smart.who.int/immunizations/bpmn/IMMZ.D2.bpmn#Determine%20required%20vaccination%28s%29%20if%20any"/>
    <dmn:decisionTable>
      <dmn:input id="input.IMMZ.D2.DT.Measles.Supplementarydose.Numberofmeaslessupplementarydosesadministered" label="Number of measles supplementary doses administered">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Measles.Supplementarydose.Numberofmeaslessupplementarydosesadministered" typeRef="string">
          <dmn:text>Count of vaccines administered (where "Vaccine type" = "Measles-containing vaccines" and "Type of dose" = "Supplementary dose")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.IMMZ.D2.DT.Measles.Supplementarydose.Measlesroutineimmunizationschedulestatus" label="Measles routine immunization schedule status">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Measles.Supplementarydose.Measlesroutineimmunizationschedulestatus" typeRef="string">
          <dmn:text>"Completed the primary vaccination series" (where "Vaccine type" = "Measles-containing vaccines")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.IMMZ.D2.DT.Measles.Supplementarydose.Timepassedsincealivevaccinewasadministered" label="Time passed since a live vaccine was administered">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Measles.Supplementarydose.Timepassedsincealivevaccinewasadministered" typeRef="string">
          <dmn:text>Today's date − latest "Date and time of vaccination" (where "Live vaccine" = TRUE)</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="output.IMMZ.D2.DT.Measles.Supplementarydose.CarePlan" label="Care Plan">
        <dmn:description>Produce a suggested Care Plan for consideration by health worker</dmn:description>
      </dmn:output>
