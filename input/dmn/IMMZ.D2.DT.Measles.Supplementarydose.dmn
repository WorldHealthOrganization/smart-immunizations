<dmn:definitions xmlns:dmn="https://www.omg.org/spec/DMN/20240513/MODEL/" namespace="http://smart.who.int/immunizations" name="IMMZ.D2.DT.Measles.Supplementarydose" id="httpsmart.who.intimmunizationsdmnIMMZ.D2.DT.Measles.Supplementarydose.dmn">
  <dmn:decision id="IMMZ.D2.DT.Measles.Supplementarydose" name="IMMZ.D2.DT.Measles.Supplementarydose">
    <dmn:decisionTable>
      <dmn:input id="input.IMMZ.D2.DT.Measles.Supplementarydose.Measles-containingvaccineMCVsupplementarydoseadministration" label="Measles-containing vaccine (MCV) supplementary dose administration">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Measles.Supplementarydose.Measles-containingvaccineMCVsupplementarydoseadministration" typeRef="string">
          <dmn:text>Measles-containing vaccine (MCV) supplementary dose administration</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="output.IMMZ.D2.DT.Measles.Supplementarydose.CarePlan" label="Care Plan">
        <dmn:description>Produce a suggested Care Plan for consideration by health worker</dmn:description>
      </dmn:output>
      <dmn:output id="output.IMMZ.D2.DT.Measles.Supplementarydose.Guidancedisplayedtohealthworker" label="Guidance displayed to health worker">
