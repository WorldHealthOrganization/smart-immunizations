          <dmn:text>Potential contraindications</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry>
          <dmn:description>Output</dmn:description>
          <dmn:text>Output</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry>
          <dmn:description>Guidance displayed to health worker</dmn:description>
          <dmn:text>Guidance displayed to health worker</dmn:text>
