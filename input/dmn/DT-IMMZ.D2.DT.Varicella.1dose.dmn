<dmn:definitions xmlns:dmn="https://www.omg.org/spec/DMN/20240513/MODEL/" namespace="http://smart.who.int/immunizations" name="DT-IMMZ.D2.DT.Varicella.1dose" id="httpsmart.who.intimmunizationsdmnDT-IMMZ.D2..03d779278f">
  <dmn:decision id="DT-IMMZ.D2.DT.Varicella.1dose" name="DT-IMMZ.D2.DT.Varicella.1dose">
    <dmn:question>Determine if the client is due for a varicella vaccination according to the national immunization schedule</dmn:question>
    <dmn:usingTask href="http://smart.who.int/immunizations/bpmn/IMMZ.D2.bpmn#Determine%20required%20vaccination%28s%29%20if%20any"/>
    <dmn:decisionTable>
      <dmn:input id="input.IMMZ.D2.DT.Varicella.1dose.Numberofvaricellaprimaryseriesdosesadministered" label="Number of varicella primary series doses administered">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Varicella.1dose.Numberofvaricellaprimaryseriesdosesadministered" typeRef="string">
          <dmn:text>Count of vaccines administered (where "Vaccine type" = "Varicella-containing vaccines" and "Type of dose" = "Primary series")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.IMMZ.D2.DT.Varicella.1dose.Clientsage" label="Clients age">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Varicella.1dose.Clientsage" typeRef="string">
          <dmn:text>Today's date − "Date of birth"</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.IMMZ.D2.DT.Varicella.1dose.Timepassedsincealivevaccinewasadministered" label="Time passed since a live vaccine was administered">
