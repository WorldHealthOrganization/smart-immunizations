        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.DAK.DT.IMMZ.D2.DT.Polio.bOPVplusIPV.Clientsage" label="Clients age">
        <dmn:inputExpression id="inputExpression.DAK.DT.IMMZ.D2.DT.Polio.bOPVplusIPV.Clientsage" typeRef="string">
          <dmn:text>Today's date − "Date of birth"</dmn:text>
