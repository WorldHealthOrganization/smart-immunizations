  <dmn:decision id="DAK.DT.IMMZ.D2.DT.Polio.bOPVplusIPV" label="IMMZ.D2  Determine required vaccination(s) if any">
    <dmn:question>IMMZ.D2  Determine required vaccination(s) if any</dmn:question>
