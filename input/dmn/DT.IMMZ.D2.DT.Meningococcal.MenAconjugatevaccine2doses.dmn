        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.DT.IMMZ.D2.DT.Meningococcal.MenAconjugatevaccine2doses.Clientsage" label="Clients age">
        <dmn:inputExpression id="inputExpression.DT.IMMZ.D2.DT.Meningococcal.MenAconjugatevaccine2doses.Clientsage" typeRef="string">
          <dmn:text>Today's date − "Date of birth"</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.DT.IMMZ.D2.DT.Meningococcal.MenAconjugatevaccine2doses.Timepassedsincelastmeningococcalvaccination" label="Time passed since last meningococcal vaccination">
        <dmn:inputExpression id="inputExpression.DT.IMMZ.D2.DT.Meningococcal.MenAconjugatevaccine2doses.Timepassedsincelastmeningococcalvaccination" typeRef="string">
          <dmn:text>Today's date − "Date and time of vaccination" (where "Vaccine type" = "Meningococcal vaccines")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="output.DT.IMMZ.D2.DT.Meningococcal.MenAconjugatevaccine2doses.CarePlan" label="Care Plan">
