        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="output.IMMZ.D2.DT.DTP.On-timestart.CarePlan" label="Care Plan">
        <dmn:description>Produce a suggested Care Plan for consideration by health worker</dmn:description>
