<dmn:definitions xmlns:dmn="https://www.omg.org/spec/DMN/20240513/MODEL/" namespace="http://smart.who.int/immunizations" name="IMMZ.D2.DT.DTP.On-timestart" id="httpsmart.who.intimmunizationsdmnIMMZ.D2.DT.DTP.On-timestart.dmn">
