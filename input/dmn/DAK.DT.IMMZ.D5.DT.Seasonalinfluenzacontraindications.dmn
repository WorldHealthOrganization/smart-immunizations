<dmn:definitions xmlns:dmn="https://www.omg.org/spec/DMN/20240513/MODEL/" namespace="http://smart.who.int/immunizations" label="IMMZ.D5 Determine vaccine(s) to be administered based on contraindications" id="DAK.DT.IMMZ.D5.DT.Seasonalinfluenzacontraindications">
  <dmn:decision id="DAK.DT.IMMZ.D5.DT.Seasonalinfluenzacontraindications" label="IMMZ.D5 Determine vaccine(s) to be administered based on contraindications">
    <dmn:question>IMMZ.D5 Determine vaccine(s) to be administered based on contraindications</dmn:question>
    <dmn:usingTask href="http://smart.who.int/immunizations/bpmn/Check.bpmn#for%20contraindications%20before%20administering%20the%20vaccine%28s%29%20due"/>
    <dmn:decisionTable id="DAK.DT.IMMZ.D5.DT.Seasonalinfluenzacontraindications">
      <dmn:input id="input.PotentialcontraindicationsDAK.DT.IMMZ.D5.DT.S163a641880." label="Potential contraindications"/>
      <dmn:rule id="rule.contra.DAK.DT.IMMZ.D5.DT.Seasonalinfluenzacon53dead5b3d">
        <dmn:inputEntry>
          <dmn:description>"Potential contraindications" = "Severe allergic reactions"</dmn:description>
          <dmn:text>The client has a history of severe allergic reactions after a previous dose or to a vaccine component</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry>
          <dmn:description>'"Immunization recommendation status" = "Further evaluation needed"</dmn:description>
          <dmn:text>Seasonal influenza vaccination could be contraindicated. Clinical judgement is required. Create a clinical note</dmn:text>
        </dmn:outputEntry>
        <dmn:outputEntry>
          <dmn:description>Do not vaccinate client with seasonal influenza if client has had a severe allergic reaction (e.g. anaphylaxis) after a previous dose or to a vaccine component.</dmn:description>
          <dmn:text>Do not vaccinate client with seasonal influenza if client has had a severe allergic reaction (e.g. anaphylaxis) after a previous dose or to a vaccine component.</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
    </dmn:decisionTable>
  </dmn:decision>
</dmn:definitions>