  <dmn:decision id="DT-IMMZ.D2.DT.Measles.Lowtransmission" name="DT-IMMZ.D2.DT.Measles.Lowtransmission">
    <dmn:question>Determine if the client is due for a measles vaccination according to the national immunization schedule</dmn:question>
    <dmn:usingTask href="http://smart.who.int/immunizations/bpmn/IMMZ.D2.bpmn#Determine%20required%20vaccination%28s%29%20if%20any"/>
    <dmn:decisionTable>
      <dmn:input id="input.IMMZ.D2.DT.Measles.Lowtransmission.Numberofmeaslesprimaryseriesdosesadministered" label="Number of measles primary series doses administered">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Measles.Lowtransmission.Numberofmeaslesprimaryseriesdosesadministered" typeRef="string">
          <dmn:text>Count of vaccines administered (where "Vaccine type" = "Measles-containing vaccines" and "Type of dose" = "Primary series")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.IMMZ.D2.DT.Measles.Lowtransmission.Clientsage" label="Clients age">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Measles.Lowtransmission.Clientsage" typeRef="string">
          <dmn:text>Today's date − "Date of birth"</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.IMMZ.D2.DT.Measles.Lowtransmission.Timepassedsincealivevaccinewasadministered" label="Time passed since a live vaccine was administered">
        <dmn:inputExpression id="inputExpression.IMMZ.D2.DT.Measles.Lowtransmission.Timepassedsincealivevaccinewasadministered" typeRef="string">
          <dmn:text>Today's date − latest "Date and time of vaccination" (where "Live vaccine" = TRUE)</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="output.IMMZ.D2.DT.Measles.Lowtransmission.CarePlan" label="Care Plan">
        <dmn:description>Produce a suggested Care Plan for consideration by health worker</dmn:description>
      </dmn:output>
      <dmn:output id="output.IMMZ.D2.DT.Measles.Lowtransmission.Guidancedisplayedtohealthworker" label="Guidance displayed to health worker">
