      <dmn:input id="input.DAK.DT.IMMZ.D2.DT.DTP.Pregnancystartingwith4doses.Clientspregnancystatus" label="Clients pregnancy status">
        <dmn:inputExpression id="inputExpression.DAK.DT.IMMZ.D2.DT.DTP.Pregnancystartingwith4doses.Clientspregnancystatus" typeRef="string">
          <dmn:text>Client's pregnancy status</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.DAK.DT.IMMZ.D2.DT.DTP.Pregnancystartingwith4doses.Timepassedsincelasttetanusanddiphtheriadosewaf964246bf0" label="Time passed since last tetanus and diphtheria dose was administered">
        <dmn:inputExpression id="inputExpression.DAK.DT.IMMZ.D2.DT.DTP.Pregnancystartingwith4doses.Timepassedsincelasttetanusanddiphtheriadosewaf964246bf0" typeRef="string">
          <dmn:text>Today's date − latest "Date and time of vaccination" (where "Vaccine type" = "Tetanus and diphtheria-containing vaccines")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="output.DAK.DT.IMMZ.D2.DT.DTP.Pregnancystartingwith4doses.CarePlan" label="Care Plan">
        <dmn:description>Produce a suggested Care Plan for consideration by health worker</dmn:description>
      </dmn:output>
