<dmn:definitions xmlns:dmn="https://www.omg.org/spec/DMN/20240513/MODEL/" namespace="http://smart.who.int/immunizations" label="IMMZ.D2 Determine required vaccination(s) if any  " id="DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening">
  <dmn:decision id="DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening" label="IMMZ.D2 Determine required vaccination(s) if any  ">
    <dmn:question>IMMZ.D2 Determine required vaccination(s) if any  </dmn:question>
    <dmn:usingTask href="http://smart.who.int/immunizations/bpmn/Determine.bpmn#if%20the%20client%20is%20due%20for%20a%20dengue%20vaccination%20according%20to%20the%20national%20immunization%20schedule"/>
    <dmn:decisionTable id="DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening">
      <dmn:input id="input.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Numberofdengueprimaryseriesdosesadministered" label="Number of dengue primary series doses administered">
        <dmn:inputExpression id="inputExpression.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Numberofdengueprimaryseriesdosesadministered" typeRef="string">
          <dmn:text>Count of vaccines administered (where "Vaccine type" = "Dengue vaccines" and "Type of dose" = "Primary series")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Clientsage" label="Clients age">
        <dmn:inputExpression id="inputExpression.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Clientsage" typeRef="string">
          <dmn:text>Today's date − "Date of birth"</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Clientsdengueserostatus" label="Clients dengue serostatus">
        <dmn:inputExpression id="inputExpression.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Clientsdengueserostatus" typeRef="string">
          <dmn:text>"Dengue serostatus"</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:input id="input.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Timepassedsincelastdenguedosewasadministered" label="Time passed since last dengue dose was administered">
        <dmn:inputExpression id="inputExpression.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Timepassedsincelastdenguedosewasadministered" typeRef="string">
          <dmn:text>Today's date − latest "Date and time of vaccination" (where "Vaccine type" = "Dengue vaccines")</dmn:text>
        </dmn:inputExpression>
      </dmn:input>
      <dmn:output id="output.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.CarePlan" label="Care Plan">
        <dmn:description>Produce a suggested Care Plan for consideration by health worker</dmn:description>
      </dmn:output>
      <dmn:output id="output.DT.IMMZ.D2.DT.Dengue.3doseswithpre-vaccinationscreening.Guidancedisplayedtohealthworker" label="Guidance displayed to health worker">
        <dmn:description>Request to communicate guidance to the health worker</dmn:description>
