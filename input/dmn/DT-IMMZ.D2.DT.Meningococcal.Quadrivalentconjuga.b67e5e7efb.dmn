  <dmn:decision id="DT-IMMZ.D2.DT.Meningococcal.Quadrivalentconj.aaa343bf35" name="DT-IMMZ.D2.DT.Meningococcal.Quadrivalentconjuga.b67e5e7efb">
    <dmn:question>Determine if the client is due for a meningococcal vaccination according to the national immunization schedule</dmn:question>
    <dmn:usingTask href="http://smart.who.int/immunizations/bpmn/IMMZ.D2.bpmn#Determine%20required%20vaccination%28s%29%20if%20any"/>
