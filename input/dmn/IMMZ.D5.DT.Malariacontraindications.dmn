<dmn:definitions xmlns:dmn="https://www.omg.org/spec/DMN/20240513/MODEL/" namespace="http://smart.who.int/immunizations" name="IMMZ.D5.DT.Malariacontraindications" id="httpsmart.who.intimmunizationsdmnIMMZ.D5.DT.Malariacontraindications.dmn">
  <dmn:decision id="IMMZ.D5.DT.Malariacontraindications" name="IMMZ.D5.DT.Malariacontraindications">
    <dmn:decisionTable>
      <dmn:input id="input.Potentialcontraindications" label="Potential contraindications" />
      <dmn:rule id="rule.contra.IMMZ.D5.DT.Malariacontraindications.1">
        <dmn:inputEntry>
          <dmn:description>"Potential contraindications" = "Hypersensitivity to vaccine components"</dmn:description>
          <dmn:text>The client has hypersensitivity to any of the vaccine components</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry>
          <dmn:description>"Immunization recommendation status" =  "Further evaluation needed"</dmn:description>
          <dmn:text>Malaria vaccination could be contraindicated. Clinical judgement is required. Create a clinical note</dmn:text>
        </dmn:outputEntry>
        <dmn:annotationEntry>
          <dmn:text>–</dmn:text>
        </dmn:annotationEntry>
        <dmn:annotationEntry>
          <dmn:text>Malaria vaccine: WHO position paper (March 2022) (26)</dmn:text>
        </dmn:annotationEntry>
        <dmn:outputEntry>
          <dmn:description>Do not vaccinate client with RTS,S/AS01 vaccine if client has severe hypersensitivity to any of the vaccine components.</dmn:description>
          <dmn:text>Do not vaccinate client with RTS,S/AS01 vaccine if client has severe hypersensitivity to any of the vaccine components.</dmn:text>
        </dmn:outputEntry>
        <dmn:annotationEntry>
          <dmn:text>–</dmn:text>
        </dmn:annotationEntry>
        <dmn:annotationEntry>
          <dmn:text>Malaria vaccine: WHO position paper (March 2022) (26)</dmn:text>
        </dmn:annotationEntry>
      </dmn:rule>
    </dmn:decisionTable>
  </dmn:decision>
